    "google cloud storage",
    "rest api"
  ],
  "author": "Gabocz",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^6.9.5",
    "axios": "^1.3.2",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.4",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemon": "^2.0.20",
    "sharp": "^0.32.3",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.4"
